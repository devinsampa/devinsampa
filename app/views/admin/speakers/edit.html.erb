<% title "Cadastro de #{@speaker.name}", false %>

<% form_for(:speaker, :url => { :action => "update" }, :method => "PUT", :html => { :multipart => true, :method => "PUT" }) do |f| %>
  <%= render :partial => "form", :locals => {:f => f} %>
  <%= f.submit "Alterar", :disable_with => 'Aguarde...' %>
<% end %>
