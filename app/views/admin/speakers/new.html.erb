<h1>New speaker</h1>

<% form_for(:speaker, @speaker, :url => {:action => "create"}, :html => { :multipart => true }) do |f| %>
  <%= f.error_messages %>
  
  <%= render({:partial => "form", :locals => {:f => f}}) %>
  
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', speakers_path %>